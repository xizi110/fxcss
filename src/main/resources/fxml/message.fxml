<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>


<fx:root maxHeight="-Infinity" maxWidth="800" minHeight="-Infinity" minWidth="100"
         style="-fx-background-color: red;"
         xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" type="HBox">
   <children>
      <Label ellipsisString="" HBox.hgrow="ALWAYS" fx:id="iconLabel" />
      <TextFlow HBox.hgrow="SOMETIMES">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" fx:id="messageText"/>
         </children>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </TextFlow>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
